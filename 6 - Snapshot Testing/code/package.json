{
    "name": "isomorphic-react",
    "version": "1.0.0",
    "description": "An isomorphic React application implementing latest development standards",
    "main": "index.js",
    "scripts": {
        "build": "cross-env webpack --config ./webpack.config.prod.babel.js",
        "postinstall": "npm install -g babel-cli@7.0.0-beta.3 cross-env@5.0.5 webpack@3.6.0",
        "start": "npm run start-prod",
        "start-dev": "cross-env NODE_ENV=development babel-node server --useServerRender=true --useLiveData=false",
        "start-prod": "npm run build && cross-env NODE_ENV=production babel-node server --useServerRender=true --useLiveData=false",
        "test": "jest",
        "test-watch": "jest --watch"
    },
    "keywords": [],
    "author": "Zendy",
    "dependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-regenerator-runtime": "^6.5.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "express": "^4.17.3",
        "express-yields": "^1.1.2",
        "fs-extra": "^4.0.3",
        "history": "^4.10.1",
        "isomorphic-fetch": "^2.2.1",
        "jest": "^24.9.0",
        "lodash": "^4.17.15",
        "optimist": "^0.6.1",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-hot-loader": "^3.1.3",
        "react-markdown": "^2.5.1",
        "react-redux": "^5.1.2",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.8",
        "react-router-server": "^4.2.3",
        "react-test-renderer": "^16.10.2",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-saga": "^0.15.6",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "webpack": "^3.12.0",
        "webpack-dev-middleware": "^1.12.2",
        "webpack-hot-middleware": "^2.25.0"
    },
    "jest": {
        "verbose": true,
        "testURL": "http://localhost/"
    }
}
